<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Hospitalzinho"
                   namespace="Hospitalzinho.Entidades">

	<class name="HospitalEndereco" table="hospital_endereco" lazy="false">
		<id name="Id" column="id" type="long">
			<generator class="sequence">
				<param name="sequence">seq_hospital_endereco</param>
			</generator>
		</id>

		<property name="CEP" column="cep" type="string" />
		<property name="Cidade" column="cidade" type="string" />
		<property name="Bairro" column="bairro" type="string" />
		<property name="Rua" column="rua" type="string" />
		<property name="Numero" column="numero" type="string" />
		<property name="Complemento" column="complemento" type="string" />
		<property name="CriadoEm" column="data_cadastro" not-null="true" />
		<property name="UltimaAlteracao" column="data_atualizacao" not-null="true" />
	</class>
</hibernate-mapping>
